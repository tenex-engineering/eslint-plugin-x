{
  "name": "@txe/eslint-plugin-x",
  "version": "0.0.1",
  "description": "ESlint Plugin X",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "npm run test",
    "ci": "npm run lint && npm run build && npm run check-format && npm run check-exports && npm run test",
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --format=esm --packages=external",
    "lint": "eslint .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "test": "vitest"
  },
  "keywords": [
    "eslint",
    "plugin",
    "x"
  ],
  "author": "Chabib N. R.",
  "license": "MIT",
  "imports": {
    "#package/*": "./dist/*"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.13.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@eslint/compat": "^1.2.2",
    "@stylistic/eslint-plugin-js": "^2.10.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.9.0",
    "@typescript-eslint/rule-tester": "^8.13.0",
    "cspell": "^8.16.0",
    "esbuild": "^0.24.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  }
}
